{
  "name": "navigation-utility",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/navigation/utility/src",
  "projectType": "library",
  "tags": ["type:util"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/navigation/utility/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/navigation/utility"],
      "options": {
        "jestConfig": "libs/navigation/utility/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
